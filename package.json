{
  "name": "news-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "format": "prettier --write \"{app,components}/**/*.{ts,tsx,scss,json}\"",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "{app,components}/**/*.{ts,tsx,scss}": [
      "npm run lint:fix",
      "npm run format"
    ]
  },
  "dependencies": {
    "next": "15.5.2",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "sass": "^1.91.0",
    "typescript": "^5"
  }
}
